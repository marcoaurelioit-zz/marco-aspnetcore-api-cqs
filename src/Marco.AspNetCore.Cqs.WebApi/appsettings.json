{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SqlServerReadOnlySettings": {
    "DefaultConnection": "Data Source=XXX,1433;Initial Catalog=XXX;User Id=XXX;Password=XXX;Application Name=MarcoAspNetCoreCqsWebApi"
  },
  "CacheConfiguration": {
    "StorageType": "Memory", // Memory, SqlServer, Redis
    "Enable": true,
